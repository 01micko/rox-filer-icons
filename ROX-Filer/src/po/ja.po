msgid "Right edge"
msgstr "右端"

#: ../Templates.ui.h:22
msgid "Left edge"
msgstr "左端"

#: ../Templates.ui.h:23
msgid "Bottom edge"
msgstr "下端"

#: ../Templates.ui.h:24
msgid "Top edge"
msgstr "上端"

